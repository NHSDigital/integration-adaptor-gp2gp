<EhrExtract classCode="EXTRACT" moodCode="EVN">
    <id root="{{ehrExtractId}}" />
    <statusCode code="COMPLETE" />
    <availabilityTime value="{{availabilityTime}}" />
    <recordTarget typeCode="RCT">
        <patient classCode="PAT">
            <id root="2.16.840.1.113883.2.1.4.1" extension="{{patientId}}" />
        </patient>
    </recordTarget>
    <author typeCode="AUT">
        <time value="{{availabilityTime}}" />
        <AgentOrgSDS classCode="AGNT">
            <agentOrganizationSDS classCode="ORG" determinerCode="INSTANCE">
                <id root="1.2.826.0.1285.0.1.10" extension="{{toOdsCode}}" />
            </agentOrganizationSDS>
        </AgentOrgSDS>
    </author>
    <destination typeCode="DST">
        <AgentOrgSDS classCode="AGNT">
            <agentOrganizationSDS classCode="ORG" determinerCode="INSTANCE">
                <id root="1.2.826.0.1285.0.1.10" extension="{{fromOdsCode}}" />
            </agentOrganizationSDS>
        </AgentOrgSDS>
    </destination>
    <component typeCode="COMP">
        <ehrFolder classCode="FOLDER" moodCode="EVN">
            <id root="{{ehrFolderId}}" />
            <statusCode code="COMPLETE" />
            <effectiveTime>
                {{{effectiveTime}}}
            </effectiveTime>
            <availabilityTime value="{{availabilityTime}}" />
            <author typeCode="AUT">
                <time value="{{availabilityTime}}" />
                <AgentOrgSDS classCode="AGNT">
                    <agentOrganizationSDS classCode="ORG" determinerCode="INSTANCE">
                        <id root="1.2.826.0.1285.0.1.10" extension="{{toOdsCode}}" />
                    </agentOrganizationSDS>
                </AgentOrgSDS>
            </author>
            {{#agentDirectory}}
            {{{agentDirectory}}}
            {{/agentDirectory}}
            {{#components}}
                {{{.}}}
            {{/components}}
        </ehrFolder>
    </component>
    <inFulfillmentOf typeCode="FLFS">
        <priorEhrRequest classCode="EXTRACT" moodCode="RQO">
            <id root="{{requestId}}" />
        </priorEhrRequest>
    </inFulfillmentOf>
    <limitation typeCode="LIMIT" inversionInd="true">
        <limitingEhrExtractSpecification classCode="OBS" moodCode="DEF">
            <id root="76C49C11-5271-11EA-9384-E83935108FD5" />
            <code code="37241000000102" codeSystem="2.16.840.1.113883.2.1.3.2.4.15" displayName="Entire record available to originator (administrative concept)" />
        </limitingEhrExtractSpecification>
    </limitation>
</EhrExtract>
