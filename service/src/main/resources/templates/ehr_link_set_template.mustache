<component typeCode="COMP"{{#isNested}} contextConductionInd="true"{{/isNested}}>
    <LinkSet classCode="OBS" moodCode="EVN">
        <id root="{{linkSetId}}" />
        <effectiveTime>
            {{#effectiveTimeLow}}<low value="{{effectiveTimeLow}}"/>{{/effectiveTimeLow}}{{^effectiveTimeLow}}<low nullFlavor="UNK"/>{{/effectiveTimeLow}}
            {{#effectiveTimeHigh}}<high value="{{effectiveTimeHigh}}"/>{{/effectiveTimeHigh}}{{^effectiveTimeHigh}}<high nullFlavor="UNK"/>{{/effectiveTimeHigh}}
        </effectiveTime>
        <availabilityTime value="{{availabilityTime}}" />
        {{#conditionNamed}}
        <conditionNamed typeCode="NAME" inversionInd="true">
            <namedStatementRef classCode="OBS" moodCode="EVN">
                <id root="{{conditionNamed}}"/>
            </namedStatementRef>
        </conditionNamed>
        {{/conditionNamed}}
        {{#relatedClinicalContent}}
        <component typeCode="COMP">
            <statementRef classCode="OBS" moodCode="EVN">
                <id root="{{.}}"/>
            </statementRef>
        </component>
        {{/relatedClinicalContent}}
        {{#clinicalStatusIsActive}}<code code="394774009" codeSystem="2.16.840.1.113883.2.1.3.2.4.15" displayName="Active Problem">{{/clinicalStatusIsActive}}{{^clinicalStatusIsActive}}<code code="394775005" codeSystem="2.16.840.1.113883.2.1.3.2.4.15" displayName="Inactive Problem">{{/clinicalStatusIsActive}}
            <originalText>{{#clinicalStatusIsActive}}A{{/clinicalStatusIsActive}}{{^clinicalStatusIsActive}}Ina{{/clinicalStatusIsActive}}ctive Problem, {{qualifier}}</originalText>
            <qualifier inverted="false">
                {{#qualifierIsMajor}}<name code="386134007" codeSystem="2.16.840.1.113883.2.1.3.2.4.15" displayName="Significant"/>{{/qualifierIsMajor}}{{^qualifierIsMajor}}<name code="394847000" codeSystem="2.16.840.1.113883.2.1.3.2.4.15" displayName="Unspecified significance"/>{{/qualifierIsMajor}}
            </qualifier>
        </code>
    </LinkSet>
</component>
{{#generateObservationStatement}}<component>
    <ObservationStatement classCode="OBS" moodCode="EVN">
        <id root="{{conditionNamed}}"/>
        <code><!--code placeholder--></code>
        <statusCode code="COMPLETE"/>
        <effectiveTime>
            {{#effectiveTimeLow}}<low value="{{effectiveTimeLow}}"/>{{/effectiveTimeLow}}{{^effectiveTimeLow}}<low nullFlavor="UNK"/>{{/effectiveTimeLow}}
            {{#effectiveTimeHigh}}<high value="{{effectiveTimeHigh}}"/>{{/effectiveTimeHigh}}{{^effectiveTimeHigh}}<high nullFlavor="UNK"/>{{/effectiveTimeHigh}}
        </effectiveTime>
        <availabilityTime value="{{availabilityTime}}" />
        {{#pertinentInfo}}
        <pertinentInformation typeCode="PERT">
            <sequenceNumber value="+1" />
            <pertinentAnnotation classCode="OBS" moodCode="EVN">
                <text>{{pertinentInfo}}</text>
            </pertinentAnnotation>
        </pertinentInformation>
        {{/pertinentInfo}}  </ObservationStatement>
</component>{{/generateObservationStatement}}