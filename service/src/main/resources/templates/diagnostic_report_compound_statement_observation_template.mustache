<component typeCode="COMP" contextConductionInd="true">
    <CompoundStatement classCode="CLUSTER" moodCode="EVN">
        <id root="{{compoundStatementObservationId}}"/> <!-- generated ID? -->
        <code code="{{observationReadCode}}" codeSystem="2.16.840.1.113883.2.1.6.2" displayName="{{observationReadCodeDisplayName}}">
            <translation code="{{observationSnomedCode}}" codeSystem="2.16.840.1.113883.2.1.3.2.4.15" displayName="{{observationSnomedCodeDisplayName}}"/>
        </code>
        <statusCode code="COMPLETE"/>
        <effectiveTime>
            <center value="{{observationEffectiveDateTime}}"/>
        </effectiveTime>
        <availabilityTime value="{{observationIssuedDate}}"/> <!-- Is this correct? -->
        <component typeCode="COMP" contextConductionInd="true">
            <ObservationStatement classCode="OBS" moodCode="EVN">
                <id root="{{observationStatementObservationId}}"/> <!-- generated ID? -->
                <code code="{{observationReadCode}}" codeSystem="2.16.840.1.113883.2.1.6.2" displayName="{{observationReadCodeDisplayName}}">
                    <translation code="{{observationSnomedCode}}" codeSystem="2.16.840.1.113883.2.1.3.2.4.15" displayName="{{observationSnomedCodeDisplayName}}"/>
                </code>
                <statusCode code="COMPLETE"/>
                <effectiveTime>
                    <center value="{{observationEffectiveDateTime}}"/>
                </effectiveTime>
                <availabilityTime value="{{observationIssuedDate}}"/> <!-- Is this correct? -->
            </ObservationStatement>
        </component>
        {{#narrativeStatements}}
            {{{.}}}
        {{/narrativeStatements}}
    </CompoundStatement>
</component>
