<part typeCode="PART">
    <Agent classCode="AGNT">
        <id root="{{agentId}}"/>
        {{#practitionerRole}}
        <code nullFlavor="UNK">
            <originalText>{{practitionerRole}}</originalText>
        </code>
        {{/practitionerRole}}
        {{#practitioner}}
        <agentPerson classCode="PSN" determinerCode="INSTANCE">
            <name>
                {{#practitionerPrefix}}
                <prefix>{{practitionerPrefix}}</prefix>
                {{/practitionerPrefix}}
                {{#practitionerGivenName}}
                <given>{{practitionerGivenName}}</given>
                {{/practitionerGivenName}}
                {{#practitionerFamilyName}}
                <family>{{practitionerFamilyName}}</family>
                {{/practitionerFamilyName}}
            </name>
        </agentPerson>
        {{/practitioner}}
        {{#organization}}
        <representedOrganization classCode="ORG" determinerCode="INSTANCE">
{{{organization}}}
        </representedOrganization>
        {{/organization}}
    </Agent>
</part>