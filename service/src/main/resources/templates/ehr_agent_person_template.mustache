<part partCode="PART">
    <Agent classCode="AGNT">
        <id root="{{practitionerId}}"/>
        <agentPerson classCode="PSN" determinerCode="INSTANCE">
            {{#practitionerRole}}
            <code nullFlavor="UNK">
                <orginalText>{{practitionerRole}}</orginalText>
            </code>
            {{/practitionerRole}}
            <name>
                {{#practitionerPrefix}}
                <prefix>{{practitionerPrefix}}</prefix>
                {{/practitionerPrefix}}
                {{#practitionerGivenName}}
                <given>{{practitionerGivenName}}</given>
                {{/practitionerGivenName}}
                {{#practitionerFamilyName}}
                <family>{{practitionerFamilyName}}</family>
                {{/practitionerFamilyName}}
            </name>
        </agentPerson>
        {{#organization}}
        <representedOrganization classCode="ORG" determiner="INSTANCE">
{{{organization}}}
        </representedOrganization>
        {{/organization}}
    </Agent>
</part>